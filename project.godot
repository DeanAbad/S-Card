; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="S-Card"
config/description="A simple 3D card mockup with an original character, Frilla Cosmos.
The texture or artwork used for the card is made with Aseprite."
run/main_scene="res://scenes/DeckScene.tscn"
config/icon="res://assets/icons/s-card_icon.png"
config/windows_native_icon="res://assets/icons/s-card_icon.ico"

[audio]

driver=""

[display]

window/size/width=900
window/tablet_driver=""
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

2d/snapping/use_gpu_pixel_snap=true
environment/default_clear_color=Color( 0.121569, 0.0509804, 0.0980392, 1 )
environment/default_environment="res://default_env.tres"
